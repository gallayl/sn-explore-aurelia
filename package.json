{
  "name": "sn-explore-aurelia",
  "version": "1.0.0-development1",
  "description": "A starter kit for building a standard navigation-style app with Aurelia and Webpack.",
  "main": "dist/app.bundle.js",
  "repository": {
    "url": "git+ssh://git@github.com/SenseNet/sn-explore-aurelia.git",
    "type": "git"
  },
  "author": "Sense/Net <snteam@sensenet.com> (http://sensenet.com/)",
  "license": "GPL-2.0",
  "engines": {
    "node": ">= 6.0.0"
  },
  "keywords": [
    "sensenet",
    "ecm",
    "javascript",
    "REST",
    "ajax",
    "API"
  ],
  "scripts": {
    "start": "nps",
    "build": "nps build development",
    "test": "nps test",
    "commit": "git-cz"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.spec\\.(ts|js)x?$",
    "setupFiles": [
      "<rootDir>/test/jest-pretest.ts"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "aurelia-(.*)": "<rootDir>/node_modules/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!**/*.spec.{js,ts}",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage-jest",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "mapCoverage": true
  },
  "devDependencies": {
    "@types/jest": "^21.1.2",
    "@types/webpack": "^3.0.13",
    "aurelia-protractor-plugin": "^1.0.5",
    "aurelia-testing": "^1.0.0-beta.3.0.1",
    "aurelia-webpack-plugin": "~2.0.0-rc.5",
    "autoprefixer": "^7.1.4",
    "awesome-typescript-loader": "^3.2.3",
    "commitizen": "^2.9.6",
    "copy-webpack-plugin": "^4.1.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "jest": "^21.2.1",
    "jest-cli": "~21.1.0",
    "json-loader": "^0.5.7",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-webpack": "^2.0.4",
    "node-sass": "^4.5.3",
    "nps": "^5.7.1",
    "nps-utils": "^1.4.0",
    "postcss-loader": "^2.0.6",
    "protractor": "^5.1.2",
    "sass-loader": "^6.0.6",
    "sn-client-js": "^3.0.0-development.1",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.0.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3",
    "url-loader": "^0.6.1",
    "wait-on": "^2.0.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.77",
    "@types/quill": "^1.3.3",
    "aurelia-animator-css": "^1.0.3",
    "aurelia-bootstrapper": "^2.1.1",
    "aurelia-framework": "^1.1.5",
    "aurelia-history-browser": "^1.1.0",
    "aurelia-loader-nodejs": "^1.0.1",
    "aurelia-loader-webpack": "^2.1.0",
    "aurelia-logging-console": "^1.0.0",
    "aurelia-pal-browser": "^1.3.0",
    "aurelia-pal-nodejs": "^1.0.0-beta.2.0.0",
    "aurelia-resize": "^1.1.2",
    "aurelia-templating": "^1.5.0",
    "aurelia-templating-binding": "^1.4.0",
    "aurelia-templating-resources": "^1.5.1",
    "aurelia-templating-router": "^1.2.0",
    "bluebird": "^3.5.0",
    "cpx": "^1.5.0",
    "font-awesome": "^4.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment-timezone": "^0.5.13",
    "quill": "^1.3.2",
    "rimraf": "^2.6.2",
    "sn-controls-aurelia": "^2.0.0-development.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
